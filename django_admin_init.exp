#!/usr/bin/env expect

set timeout -1
spawn /opt/graphite/bin/django-admin createsuperuser

expect "Username (leave blank to use 'root'):" {
  send "root\r"
}

expect "Email address:" {
  send "root@example.com\r"
}

expect "Password:" {
  send "password\r"
}

expect "Password *:" {
  send "password\r"
}

expect "Superuser created successfully"
